{
  "name": "dorfmap",
  "version": "1.0.3",
  "description": "",
  "main": "webpack.config.js",
  "repository": "https://github.com/chaosdorf/dorfmap",
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "webpack-dashboard -- webpack-dev-server --inline --content-base public/ --history-api-fallback --quiet",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.map && rimraf public/index.html",
    "build:webpack": "npm run cleanup && webpack --bail --profile --json > reports/webpack-stats.json",
    "build:perf": "webpack-bundle-analyzer reports/webpack-stats.json public/ --mode static -s gzip --report reports/WebpackBundleReport.html --no-open",
    "build": "npm run build:webpack && npm run build:perf",
    "test": "flow && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && PRIMUS=http://dorfmapserver.chaosdorf.space:3001 BASE_HOST= NODE_ENV=production npm run build; sh scripts/dev/copy-to-server",
    "lint": "eslint src/**/*.js*"
  },
  "author": "marudor",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "axios": "^0.18.0",
    "classcat": "^3.0.2",
    "immutable": "^4.0.0-rc.9",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-promise": "^0.5.3",
    "reselect": "^3.0.1",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-1": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-flow-react-proptypes": "^23.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-joblift": "^1.3.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "0.69.0",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.12.0",
    "redux-unhandled-action": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^3",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^2"
  }
}
