{
  "name": "dorfmap",
  "version": "1.0.3",
  "description": "",
  "repository": "https://github.com/chaosdorf/dorfmap",
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "webpack-dev-server",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.map && rimraf public/index.html",
    "build:webpack": "npm run cleanup && webpack",
    "build": "npm run build:webpack",
    "build:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json public/ --mode static -s gzip --report ./BundleReport.html --no-open",
    "test": "flow && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && PRIMUS='http://dorfmapserver.chaosdorf.space:3001' BASE_HOST='' NODE_ENV=production npm run build && sh scripts/dev/copy-to-server",
    "lint": "eslint src/**/*.js*"
  },
  "author": "marudor",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "@material-ui/core": "^1.3.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classcat": "^3.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-promise": "^0.6.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-flow": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-1": "^7.0.0-beta.51",
    "babel-eslint": "8.2.5",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-flow-react-proptypes": "^24.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-config-joblift": "^1.3.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "flow-bin": "0.75.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "parcel-bundler": "^1.9.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.7",
