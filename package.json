{
  "name": "dorfmap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rimraf dist/static && ln -s . dist/static",
    "lint": "vue-cli-service lint",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "test": "tsc && npm run lint",
    "deploy": "npm run lint && SOCKET_URL='http://dorfmapserver.chaosdorf.space:3001' npm run build && sh scripts/dev/copy-to-server"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.7.10",
    "core-js": "^2.6.5",
    "he": "^1.2.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.2.1",
    "vuex": "^3.1.1",
    "vuex-module-decorators": "^0.9.9"
  },
  "devDependencies": {
    "@types/he": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "typescript": "^3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
