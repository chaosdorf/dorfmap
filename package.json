{
  "name": "dorfmap",
  "version": "1.0.3",
  "description": "",
  "repository": "https://github.com/chaosdorf/dorfmap",
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "parcel src/index.html -d public",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.map && rimraf public/index.html",
    "build:parcel": "npm run cleanup && parcel build src/index.html -d public",
    "build": "npm run build:parcel",
    "test": "flow && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && PRIMUS='http://dorfmapserver.chaosdorf.space:3001' BASE_HOST='' NODE_ENV=production npm run build && sh scripts/dev/copy-to-server",
    "lint": "eslint src/**/*.js*"
  },
  "author": "marudor",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.46",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classcat": "^3.0.2",
    "classnames": "^2.2.0",
    "immutable": "^4.0.0-rc.9",
    "material-ui": "^1.0.0-beta.44",
    "prop-types": "^15.5.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "redux-promise": "^0.5.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-flow-react-proptypes": "^24.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-joblift": "^1.3.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "flow-bin": "0.71.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.8.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "prettier": "^1.12.1",
    "redux-unhandled-action": "^1.3.0",
    "rimraf": "^2.6.2"
  }
}
