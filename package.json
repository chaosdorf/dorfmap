{
  "name": "dorfmap",
  "version": "1.0.3",
  "description": "",
  "repository": "https://github.com/chaosdorf/dorfmap",
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "webpack-dev-server",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.LICENSE && rimraf public/*.js.map && rimraf public/index.html",
    "build:webpack": "npm run cleanup && webpack",
    "build": "npm run build:webpack",
    "build:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json public/ --mode static -s gzip --report ./BundleReport.html --no-open && rm webpack-stats.json",
    "test": "tsc && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && SOCKET_URL='http://dorfmapserver.chaosdorf.space:3001' BASE_HOST='' NODE_ENV=production npm run build && sh scripts/dev/copy-to-server",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src"
  },
  "author": "marudor",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "axios": "0.19.0",
    "core-js": "^3.6.4",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "unstated-next": "^1.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/babel__core": "^7.1.6",
    "@types/core-js": "^2.5.3",
    "@types/eslint": "^6.1.9",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node-sass": "^4.11.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/prettier": "^1.19.1",
    "@types/raf": "^3.4.0",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/rimraf": "^3.0.0",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.10",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cache-loader": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-marudor": "^7.3.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-testing-library": "^3.0.1",
    "html-webpack-plugin": "^4.0.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  }
}
