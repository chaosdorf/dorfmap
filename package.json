  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "webpack-dev-server",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.LICENSE && rimraf public/*.js.map && rimraf public/index.html",
    "build:webpack": "npm run cleanup && webpack",
    "build": "npm run build:webpack",
    "build:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json public/ --mode static -s gzip --report ./BundleReport.html --no-open && rm webpack-stats.json",
    "test": "tsc && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && SOCKET_URL='http://dorfmapserver.chaosdorf.space:3001' BASE_HOST='' NODE_ENV=production npm run build && sh scripts/dev/copy-to-server",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src"
  },
  "author": "marudor",
  "license": "MIT",
