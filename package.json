{
  "name": "dorfmap",
  "version": "1.0.3",
  "description": "",
  "repository": "https://github.com/chaosdorf/dorfmap",
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "dorfmap"
  ],
  "contributors": [],
  "bugs": "https://github.com/chaosdorf/dorfmap/issues",
  "scripts": {
    "dev:all": "node scripts/devAll",
    "dev:server": "nodemon --watch \"backend/**\" --exec \"perl backend/usr/lib/dorfmap/index.pl daemon\"",
    "dev": "webpack-dev-server",
    "cleanup": "rimraf public/*.css && rimraf public/*.js && rimraf public/*.js.LICENSE && rimraf public/*.js.map && rimraf public/index.html",
    "build:webpack": "npm run cleanup && webpack",
    "build": "npm run build:webpack",
    "build:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json public/ --mode static -s gzip --report ./BundleReport.html --no-open && rm webpack-stats.json",
    "test": "flow && npm run lint",
    "test:travis": "npm run test && NODE_ENV=production npm run build",
    "deploy": "npm run lint && SOCKET_URL='http://dorfmapserver.chaosdorf.space:3001' BASE_HOST='' NODE_ENV=production npm run build && sh scripts/dev/copy-to-server",
    "lint": "eslint src/**/*.js*"
  },
  "author": "marudor",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@material-ui/core": "^3.6.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "raf": "^3.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
