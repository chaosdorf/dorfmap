{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "globals": {
    "_": false
  },
  "rules": {
    "quotes": [2, "single"],
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "consistent-return": 0,
    "no-loop-func": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "camelcase": 0,
    "eqeqeq": 0,
    "no-new": 0,
    "no-debugger": 0,
    "no-trailing-spaces": 0,
    "no-fallthrough": 0,
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-in-parens": [2, "never"],
    "wrap-iife": 1,
    "generator-star-spacing": [2, "after"]
  },
  "plugins": [
    "react"
  ]
}
