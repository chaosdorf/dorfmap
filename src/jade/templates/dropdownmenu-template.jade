material-dialog
  material-tabs(selected="selectedIndex" noink, style="width:30em;")
    material-tab(ng-repeat="menu in dropdownData",label="{{menu.name}}")
      div(style="height:{{dropdownData.height}}px;")
        material-button.dropdownButton(ng-repeat="d in menu.entries",ng-click="menu.function(d.name, hide)") {{d.name}}
