{
  "presets": ["react", ["es2015", { "modules": false }], "stage-2"],
  "plugins": [
    "transform-decorators-legacy",
    "lodash"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "transform-remove-console",
        "transform-remove-debugger"
      ]
    }
  }
}
