	.overview {
		background-image: url(/map.png);
		width: 2202px;
		height: 648px;
		position: absolute;
	}

	.overview > * {
		/* overflow: hidden; */
	}

	.overview > * > .popup {
		visibility: hidden;
		position: absolute;
		top: 10px;
		left: 10px;
		z-index: 1;
		border: 1px solid gray;
		background-color: white;
		opacity: .90;
		min-width: 200px;
	}
	
	.overview > *:hover > .popup,
	.overview > * > .popup:hover {
		visibility: visible;
	}

/*
	.overview > *:hover {
		background-color: gray;
		opacity: .20;
	}
*/

	.overview .popup a {
		color: #000099;
		text-decoration: none;
	}

	.overview .open {
		background-color: green;
		opacity: .50;
	}

